<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace = "com.ztgg.ecommerce.dao.LocalAuthDao">
 	<select id = "queryLocalAuth" resultType = "com.ztgg.ecommerce.entity.LocalAuth">
 		SELECT local_auth_id, user_id, username, password, time_created, time_updated
 		FROM tb_local_auth
 		ORDER BY local_auth_id
 	</select>
 </mapper>